{
  "name": "autotelic-journey",
  "private": true,
  "description": "Autotelic Journey",
  "version": "0.1.0",
  "author": "Alexandre Lim <alexandre.lim@abbeal.com>",
  "bugs": {
    "url": "https://github.com/Wraithraiser/autotelic-journey/issues"
  },
  "dependencies": {
    "gatsby": "^2.0.8",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "typeface-merriweather": "0.0.43",
    "typeface-montserrat": "0.0.43"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.14.2"
  },
  "homepage": "https://autotelicjourney.netlify.com/",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write \"{gatsby-*.js,src/**/*.{js,jsx,json,css}}\"",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "gatsby-*.js,src/**/*.{js,jsx,json,css}": [
      "yarn format",
      "git add"
    ]
  }
}
